#include <iostream>
using namespace std;
int main() {
	int n, m;
	cin >> n >> m;
	int a[100][100];
	for (int i = 0; i < n; i++) {
		for (int s = 0; s < m; s++) {
			cin >> a[i][s];
		}
	}
	int max = a[0][0], maxi = 0, maxs = 0;
	for (int i = 0; i < n; i++) {
		for (int s = 0; s < m; s++) {
			if (a[i][s] > max) {
				max = a[i][s];
				maxi = i;
				maxs = s;
			}
		}
	}
	cout << maxi << " " << maxs;
	return 0;
}
