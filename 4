#include <iostream>
using namespace std;
int main() {
    int n;
    cin >> n;
    int a[100][100];
    for (int i = 0; i < n; ++i) {
        int b = i + 1;
        for (int j = 0; j < n; ++j) {
            if (j <= i)
                a[i][j] = --b;
            else
                a[i][j] = ++b;
            cout << a[i][j] << " ";
        }
        cout << endl;
    }
    return 0;
}
